<Page x:Class="PostirushkiApp.Views.Pages.ActiveWashPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PostirushkiApp.Views.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ActiveWashPage">

    <Grid>
        <DataGrid MouseDoubleClick="DataGrid_MouseDoubleClick" Grid.Row="1" Margin="0 0 0 0">
            <DataGrid.Columns>
                <DataGridTextColumn Header="№"></DataGridTextColumn>
                <DataGridTextColumn Header="Номер стиральной машины"></DataGridTextColumn>
                <DataGridTextColumn Header="Порошок"></DataGridTextColumn>
                <DataGridTextColumn Header="Сушка"></DataGridTextColumn>
                <DataGridTextColumn Header="Сотрудник"></DataGridTextColumn>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Page>
